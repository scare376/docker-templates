  <Name>promidius-mangatagger</Name>
  <Repository>promidius/manga-tagger:nightly</Repository>
  <Registry>https://hub.docker.com/r/promidius/manga-tagger/</Registry>
  <Network>bridge</Network>
  <MyIP/>
